{
    "name": "Speed/Trade",
    "type": "library",
    "description": "Logging for PHP 5.3",
    "keywords": ["blog", "msss"],
    "homepage": "http://github.com/kerrer/blog",
    "license": "MIT",
    "authors": [
        {
            "name": "max",
            "email": "maxkerrer@126.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"

    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/geraintluff/jsv4-php.git"
        }
    ],
    "require": {
        "doctrine/orm": "2.4.*",
        "symfony/yaml": "2.*",
        "symfony/dependency-injection": "2.*",
        "doctrine/data-fixtures": "1",
        "apache/log4php": "2.3.0",
        "swiftmailer/swiftmailer": "@stable",
        "videlalvaro/php-amqplib": "v2.1.0",
        "cboden/ratchet": "0.3.*",
        "phpunit/phpunit": "4.1.*",
        "geraintluff/jsv4":"*",
        "justinrainbow/json-schema":"~1.3"
    },
    "autoload": {
        "psr-0": {
            "": "src/",
            "MyApp": "src"
        },
        "psr-4": {
            "": "libs/"
        }
    }
}
